<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Vinotec</title>
    <meta http-equiv="Content-Type" content="text/html;charset=utf-8" />
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta http-equiv="Expires" content="0">
    <meta http-equiv="Cache-Control" content="no-cache">
    <meta http-equiv="Cache-Control" content="no-store">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link rel="icon" href="~/favicon.ico" />
    <link rel="shortcut icon" href="~/favicon.ico" />
    <link rel="stylesheet" href="~/font-awesome/css/font-awesome.min.css" />

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/lib/jqGrid/css/ui.jqgrid-bootstrap.css" />

    <link rel="stylesheet" href="~/lib/layui/css/layui.css" />
    @RenderSection("Css", required: false)
    <environment names="Development">
        <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/css/window.css" asp-append-version="true"/>
    </environment>
    <environment names="Staging,Production">
        <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/css/window.min.css" asp-append-version="true"/>
    </environment>
    <script src="~/lib/layui/layui.js"></script>
</head>
<body>

    @RenderBody()

    <environment names="Development">
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="~/lib/jqGrid/js/i18n/grid.locale-cn.js"></script>
        <script src="~/lib/jqGrid/js/jquery.jqGrid.min.js"></script>
        <script src="~/js/vino/vino.page.js" asp-append-version="true"></script>
        <script src="~/js/vino/vino.from.js" asp-append-version="true"></script>
        <script src="~/js/vino/vino.form.verify.js" asp-append-version="true"></script>
        <script src="~/js/vino/vino.ajax.js" asp-append-version="true"></script>
        <script src="~/js/vino/vino.grid.js" asp-append-version="true"></script>
        <script src="~/js/vino/vino.grid.ext.js" asp-append-version="true"></script>
        <script src="~/js/window.js" asp-append-version="true"></script>
    </environment>
    <environment names="Staging,Production">
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="~/lib/jqGrid/js/i18n/grid.locale-cn.js"></script>
        <script src="~/lib/jqGrid/js/jquery.jqGrid.min.js"></script>
        <script src="~/js/vino/vino.page.js" asp-append-version="true"></script>
        <script src="~/js/vino/vino.from.js" asp-append-version="true"></script>
        <script src="~/js/vino/vino.form.verify.js" asp-append-version="true"></script>
        <script src="~/js/vino/vino.ajax.js" asp-append-version="true"></script>
        <script src="~/js/vino/vino.grid.js" asp-append-version="true"></script>
        <script src="~/js/vino/vino.grid.ext.js" asp-append-version="true"></script>
        <script src="~/js/window.min.js" asp-append-version="true"></script>
    </environment>

    @RenderSection("Scripts", required: false)
</body>
</html>
