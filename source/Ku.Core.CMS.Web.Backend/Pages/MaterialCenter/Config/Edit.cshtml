@model Ku.Core.CMS.Web.Backend.Pages.MaterialCenter.Config.EditModel
@{
    Layout = "_FormLayout";
}
<div class="layui-fluid">
    <div class="layui-card">
        <div class="layui-card-header">素材中心相关配置</div>
        <div class="layui-card-body">
            @using (Html.BeginForm<EditModel>(""))
            {
            @Html.InputFor(x => x.Dto.PictureSavePath)
            @Html.InputFor(x => x.Dto.PictureUrl)
            @Html.InputFor(x => x.Dto.FileSavePath)
            @Html.InputFor(x => x.Dto.FileSiteUrl)
            <div class="layui-form-item">
                <div class="layui-input-block">
                    <action auth-code="" text="确认修改" lay-submit="" css="layui-btn-default" type="submit" />
                </div>
            </div>
            }
        </div>
    </div>
</div>

